{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "description": "Age",
  "type": "object",
  "properties": {
    "credentialSubject": {
      "type": "object",
      "properties": {
        "email": {
          "type": "string",
          "format": "email"
        },
        "userName": {
          "type": "string"
        },
        "age": {
          "type": "integer"
        }
      },
      "required": ["email", "userName", "age"],
      "additionalProperties": true
    }
  },
  "required": ["credentialSubject"],
  "additionalProperties": true
}
